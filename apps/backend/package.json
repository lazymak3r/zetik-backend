{
  "name": "@zetik/backend",
  "version": "0.0.1",
  "description": "Zetik backend API",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prestart": "pnpm migration:run",
    "prestart:dev": "pnpm migration:run",
    "prestart:debug": "pnpm migration:run",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest -i --config test/jest-e2e.ts",
    "test:payments": "jest src/payments/payments.service.spec.ts",
    "test:payments-e2e": "jest test/payments.e2e-spec.ts -i --config test/jest-e2e.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "pnpm typeorm -d src/data-source-migration.ts migration:generate",
    "migration:create": "pnpm typeorm migration:create",
    "migration:run": "pnpm typeorm -d src/data-source-migration.ts migration:run",
    "migration:revert": "pnpm typeorm -d src/data-source-migration.ts migration:revert",
    "generate:crash-seeds": "ts-node -r tsconfig-paths/register scripts/generate-crash-seeds.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.876.0",
    "@zetik/common": "workspace:*",
    "@zetik/shared-entities": "workspace:*",
    "@fireblocks/ts-sdk": "^12.1.0",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.1",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.1",
    "@socket.io/redis-adapter": "^8.3.0",
    "@solana/web3.js": "^1.98.2",
    "@types/ioredis": "^4.28.10",
    "@types/multer": "^2.0.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/qrcode": "^1.5.5",
    "@types/swagger-stats": "^0.95.11",
    "bignumber.js": "^9.3.0",
    "bs58": "^6.0.0",
    "bullmq": "^5.53.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.6.0",
    "env-var": "7.5.0",
    "ethers": "^6.14.3",
    "express": "^5.1.0",
    "form-data": "^4.0.2",
    "geoip-lite": "^1.4.10",
    "google-auth-library": "^9.15.1",
    "ioredis": "^5.6.1",
    "jsrsasign": "^11.1.0",
    "mailgun.js": "^12.0.2",
    "otplib": "^12.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.0",
    "qrcode": "^1.5.4",
    "redis": "^5.8.3",
    "redlock": "5.0.0-beta.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "swagger-stats": "^0.99.7",
    "tweetnacl": "^1.0.3",
    "twilio": "^5.10.3",
    "typeorm": "0.3.25",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/geoip-lite": "^1.4.4",
    "axios": "^1.10.0",
    "socket.io-client": "^4.8.1",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0"
  }
}
