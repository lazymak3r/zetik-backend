{
  "name": "@zetik/monorepo",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "watch": "turbo watch build -F @zetik/common -F @zetik/shared-entities",
    "build": "turbo build",
    "build:backend": "turbo build -F @zetik/backend",
    "build:admin": "turbo build -F @zetik/admin-panel",
    "build:crash": "turbo build -F @zetik/crash-service",
    "build:fireblocks": "turbo build -F @zetik/fireblocks-service",
    "build:currency-rate": "turbo build -F @zetik/currency-rate-service",
    "build:bonus": "turbo build -F @zetik/bonus-service",
    "build:bet-feed": "turbo build -F @zetik/bet-feed-service",
    "build:race": "turbo build -F @zetik/race-service",
    "build:services": "pnpm build:backend && pnpm build:crash && pnpm build:fireblocks && pnpm build:currency-rate && pnpm build:bonus && pnpm build:bet-feed && pnpm build:race",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start backend",
    "start:backend": "nest start backend",
    "start:backend:cluster": "NODE_CLUSTER_ENABLED=true NODE_CLUSTER_WORKERS=auto node apps/backend/dist/cluster.js",
    "start:admin": "nest start admin-panel",
    "start:crash": "nest start crash-service",
    "start:fireblocks": "nest start fireblocks-service",
    "start:currency-rate": "nest start currency-rate-service",
    "start:bonus": "nest start bonus-service",
    "start:bet-feed": "nest start bet-feed-service",
    "start:race": "nest start race-service",
    "start:dev": "nest start backend --watch",
    "start:admin:dev": "nest start admin-panel --watch",
    "start:admin-client:dev": "pnpm -F frontend/admin-panel start",
    "start:crash:dev": "nest start crash-service --watch",
    "start:fireblocks:dev": "nest start fireblocks-service --watch",
    "start:currency-rate:dev": "nest start currency-rate-service --watch",
    "start:bonus:dev": "nest start bonus-service --watch",
    "start:bet-feed:dev": "nest start bet-feed-service --watch",
    "start:race:dev": "nest start race-service --watch",
    "start:debug": "nest start backend --debug --watch",
    "start:prod": "pnpm -F @zetik/backend start:prod",
    "start:admin:prod": "pnpm -F @zetik/admin-panel start:prod",
    "start:all:dev": "concurrently \"pnpm run start:dev\" \"pnpm run start:admin:dev\"",
    "start:all:services": "concurrently \"pnpm start:crash:dev\" \"pnpm start:fireblocks:dev\" \"pnpm start:currency-rate:dev\" \"pnpm start:bonus:dev\" \"pnpm start:bet-feed:dev\" \"pnpm start:race:dev\"",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "docker:logs": "docker-compose logs -f",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "pnpm -r --aggregate-output test",
    "test:watch": "pnpm -r test:watch",
    "test:cov": "pnpm -r test:cov",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "seed": "pnpm -r seed",
    "pretest:e2e": "node apps/backend/scripts/setup-test-db.js",
    "test:e2e": "pnpm -F @zetik/backend test:e2e",
    "test:db": "ts-node scripts/test-db-connection.ts",
    "db:setup": "./apps/backend/scripts/setup-database.sh",
    "db:test": "pnpm run test:db",
    "test:report": "node apps/backend/scripts/generate-test-report.js",
    "test:all": "pnpm test && pnpm test:e2e && pnpm test:report",
    "test:crash:simulator": "cd apps/backend && node src/games/crash/tests/crash-client-simulator.js",
    "generate:crash-seeds": "pnpm -F @zetik/backend generate:crash-seeds",
    "db:migration:backend:run": "pnpm -F @zetik/backend migration:run",
    "db:migration:backend:revert": "pnpm -F @zetik/backend migration:revert",
    "simulation:run": "./scripts/run-simulation-suite.sh",
    "simulation:quick": "cd apps/backend && ts-node -r tsconfig-paths/register scripts/statistical-simulation-suite.ts 10000",
    "simulation:full": "./scripts/run-simulation-suite.sh 10000000",
    "prepare": "husky || true",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@types/cli-progress": "^3.11.6",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.10",
    "@types/supertest": "^6.0.3",
    "@vercel/style-guide": "^6.0.0",
    "chalk": "^5.6.2",
    "cli-progress": "^3.12.0",
    "concurrently": "^9.2.0",
    "dotenv": "^16.6.1",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,md}": [
      "node scripts/check-no-russian.js"
    ],
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,json,md}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@10.15.0"
}
