{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "projects": {
    "admin-panel": {
      "type": "application",
      "root": "apps/admin-panel",
      "entryFile": "main",
      "sourceRoot": "apps/admin-panel/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin-panel/tsconfig.app.json",
        "webpackConfigPath": "apps/admin-panel/webpack.config.js"
      }
    },
    "backend": {
      "type": "application",
      "root": "apps/backend",
      "entryFile": "main",
      "sourceRoot": "apps/backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/backend/tsconfig.app.json"
      }
    },
    "bonus-service": {
      "type": "application",
      "root": "apps/bonus-service",
      "entryFile": "main",
      "sourceRoot": "apps/bonus-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bonus-service/tsconfig.app.json"
      }
    },
    "crash-service": {
      "type": "application",
      "root": "apps/crash-service",
      "entryFile": "crash-service/src/main",
      "sourceRoot": "apps/crash-service/src",
