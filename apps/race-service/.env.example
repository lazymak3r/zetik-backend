# Race Service Configuration
# Port for the race service (default: 4005)
RACE_SERVICE_PORT=4005

# Node Environment
NODE_ENV=development

# Database Configuration (shared with main backend)
DB_HOST=localhost
DB_PORT=15432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=postgres

# Redis Configuration (shared with main backend)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Optional: Redis cluster mode
REDIS_CLUSTER_ENABLED=false
REDIS_CLUSTER_NODES=

# Optional: Logging level
LOG_LEVEL=debug
