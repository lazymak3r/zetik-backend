{
  "name": "@zetik/admin-panel",
  "version": "0.0.1",
  "description": "Zetik admin panel",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "nest start -p tsconfig.app.json",
    "watch": "nest start -p tsconfig.app.json --watch",
    "start:dev": "nest start -p tsconfig.app.json --watch",
    "start:debug": "nest start -p tsconfig.app.json --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config test/jest-e2e.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.876.0",
    "@zetik/common": "workspace:*",
    "@zetik/shared-entities": "workspace:*",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^16.6.0",
    "env-var": "7.5.0",
    "express": "^5.1.0",
    "ioredis": "^5.6.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.24"
  }
}
